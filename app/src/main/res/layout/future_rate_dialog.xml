<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="walletNormalModel"
            type="Object" />
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/transparent"
        android:gravity="center"
        android:orientation="vertical">


        <LinearLayout
            style="@style/ButtonTanChuang"
            android:layout_width="273dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="@dimen/udesk_20"
            android:orientation="vertical">

            <com.black.base.widget.SpanTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/text_size_16"
                android:gravity="center"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="合约费率" />

            <com.black.base.widget.SpanTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/text_size_16"
                android:textSize="@dimen/text_size_12"
                android:textColor="@color/T2"
                android:layout_weight="1"
                android:text="流动性提供方费率(Maker)"
                android:textStyle="bold"/>
            <com.black.base.widget.SpanTextView
                android:id="@+id/one"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/text_size_9"
                android:textSize="@dimen/text_size_12"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="+0.19%" />
            <com.black.base.widget.SpanTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/text_size_16"
                android:textSize="@dimen/text_size_12"
                android:textColor="@color/T2"
                android:layout_weight="1"
                android:text="流动性提供方费率(Maker)" />
            <com.black.base.widget.SpanTextView
                android:id="@+id/two"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/text_size_9"
                android:textSize="@dimen/text_size_12"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="+0.19%" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/text_size_16"
                android:orientation="horizontal">
            <com.black.base.widget.SpanTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_12"
                android:textColor="@color/T13"
                android:text="我的VIP等级:  VIP" />
                <com.black.base.widget.SpanTextView
                    android:id="@+id/three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_size_12"
                    android:textColor="@color/T13"
                    android:text="0" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:orientation="vertical">
            <com.black.base.widget.SpanTextView
                android:id="@+id/four"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_12"
                android:layout_marginTop="@dimen/text_size_14"
                android:maxLines="1"
                android:textColor="@color/T13"
                android:text="合约阶梯费率VIP+1" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/T13"/>
            </LinearLayout>
            <com.black.base.widget.SpanTextView
                android:id="@+id/btn_cancel"
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:gravity="center"
                android:background="@drawable/anniu"
                android:layout_marginTop="@dimen/text_size_16"
                android:textColor="@color/white"
                android:text="知道了"
                />


        </LinearLayout>
    </LinearLayout>
</layout>