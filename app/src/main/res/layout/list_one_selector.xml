<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="viewKLineQuotaSelectorModel"
            type="Object" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>

        <LinearLayout
            style="@style/ButtonTanChuang"
            android:layout_width="60dp"
            android:layout_marginRight="@dimen/text_size_10"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center">



           <ImageView
               android:id="@+id/maimai"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:padding="@dimen/text_size_10"
               android:src="@drawable/maimai"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/L1_ALPHA30" />

            <ImageView
                android:id="@+id/mairu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/text_size_10"
                android:src="@drawable/mairu"/>
            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/L1_ALPHA30" />

            <ImageView
                android:id="@+id/maichu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/text_size_10"
                android:src="@drawable/maichu"/>
        </LinearLayout>

    </LinearLayout>
</layout>