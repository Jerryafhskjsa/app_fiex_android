<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

        <variable
            name="walletDetailModel"
            type="Object" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/B2">


        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/B2">

            <com.google.android.material.appbar.AppBarLayout
                android:id="@id/app_bar_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/transparent"
                android:elevation="12dp"
                android:theme="@style/AppTheme"
                app:elevation="12dp">
                <FrameLayout
                    android:id="@id/header_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="44dp"
                        android:layout_alignParentTop="true">

                        <ImageButton
                            android:id="@+id/action_bar_back"
                            android:layout_width="wrap_content"
                            android:layout_height="54dp"
                            android:layout_gravity="center_vertical"
                            android:background="@null"
                            android:contentDescription="@null"
                            android:onClick="onBackClick"
                            android:paddingLeft="15dp"
                            android:paddingRight="15dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/btn_back_faction" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            android:orientation="horizontal">

                            <com.black.base.widget.SpanTextView
                                android:id="@+id/area_choose"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:layout_marginTop="15dp"
                                android:layout_marginBottom="15dp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/shortcut_area"
                                android:textColor="@color/T1"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="bold"
                                android:drawableRight="@drawable/icon_deep"/>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="28dp"
                            android:layout_gravity="right"
                            android:orientation="horizontal">

                            <com.black.base.widget.SpanTextView
                                android:id="@+id/rate"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:layout_marginTop="22dp"
                                android:layout_marginBottom="15dp"
                                android:layout_marginRight="@dimen/text_size_5"
                                android:layout_gravity="center_vertical"
                                android:text="CNY"
                                android:textColor="@color/T1"
                                android:textSize="@dimen/text_size_16"
                                android:textStyle="bold"
                                />
                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="20dp"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:background="@drawable/btn_exchange"/>
                            <ImageView
                                android:id="@+id/settings"
                                android:layout_width="wrap_content"
                                android:layout_height="15dp"
                                android:layout_marginRight="@dimen/text_size_5"
                                android:layout_marginTop="@dimen/text_size_14"
                                android:background="@drawable/c2c">

                            </ImageView>

                        </LinearLayout>
                    </FrameLayout>

                </FrameLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/default_padding"
                    android:background="@color/B1" />
            </com.google.android.material.appbar.AppBarLayout>

            <com.black.lib.refresh.QRefreshLayout
                android:id="@+id/refresh_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_behavior="@string/appbar_scrolling_view_behavior">

                <com.black.base.widget.ObserveScrollView
                    android:id="@+id/scroll_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/B2"
                    android:fadingEdge="none"
                    android:fillViewport="true"
                    android:overScrollMode="never"
                    android:scrollbars="none">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/B2"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="28dp"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:layout_marginBottom="@dimen/text_size_10"
                                android:layout_marginLeft="@dimen/text_size_20"
                                android:background="@drawable/bg_c2c_header"
                                android:orientation="horizontal">

                                <com.black.base.widget.SpanCheckedTextView
                                    android:id="@+id/c2c_one_key"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/bg_c2c_tab_header"
                                    android:gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/my_buy"
                                    android:textColor="@color/btn_buy_check"
                                    android:textSize="@dimen/text_size_14"
                                    android:textStyle="bold" />

                                <com.black.base.widget.SpanCheckedTextView
                                    android:id="@+id/c2c_customer"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/bg_c2c_tab_header"
                                    android:gravity="center"
                                    android:checked="true"
                                    android:maxLines="1"
                                    android:text="@string/my_sale"
                                    android:textColor="@color/btn_buy_check"
                                    android:textSize="@dimen/text_size_14"
                                    android:textStyle="bold" />

                            </LinearLayout>
                            <ImageView
                                android:id="@+id/person"
                                android:layout_width="30dp"
                                android:layout_height="35dp"
                                android:layout_marginTop="7dp"
                                android:layout_marginLeft="130dp"
                                android:background="@drawable/btn_person">
                            </ImageView>
                            <ImageView
                                android:id="@+id/bills"
                                android:layout_width="30dp"
                                android:layout_height="35dp"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:background="@drawable/btn_c2c_bills">
                            </ImageView>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/text_size_20"
                            android:layout_marginLeft="@dimen/text_size_20"
                            android:layout_marginTop="@dimen/text_size_10"
                            android:orientation="horizontal">

                            <com.black.base.widget.SpanCheckedTextView
                                android:id="@+id/one"
                                style="@style/ButtonGrayLight"
                                android:layout_width="80dp"
                                android:layout_height="wrap_content"
                                android:checked="true"
                                android:gravity="center"
                                android:text="USDT" />

                            <View
                                android:layout_width="5dp"
                                android:layout_height="@dimen/line" />

                            <com.black.base.widget.SpanCheckedTextView
                                android:id="@+id/two"
                                style="@style/ButtonGrayLight"
                                android:layout_width="80dp"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="BTC" />
                            <View
                                android:layout_width="5dp"
                                android:layout_height="@dimen/line" />

                            <com.black.base.widget.SpanCheckedTextView
                                android:id="@+id/three"
                                style="@style/ButtonGrayLight"
                                android:layout_width="80dp"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="BUSD" />
                            <View
                                android:layout_width="5dp"
                                android:layout_height="@dimen/line" />

                            <com.black.base.widget.SpanCheckedTextView
                                android:id="@+id/four"
                                style="@style/ButtonGrayLight"
                                android:layout_width="80dp"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="BNB" />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/text_size_20"
                            android:layout_marginRight="@dimen/text_size_10"
                            android:orientation="horizontal">

                            <com.black.base.widget.SpanCheckedTextView
                                android:id="@+id/five"
                                style="@style/ButtonGrayLight"
                                android:layout_width="80dp"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="DOGE" />
                            <View
                                android:layout_width="5dp"
                                android:layout_height="@dimen/line" />

                            <com.black.base.widget.SpanCheckedTextView
                                android:id="@+id/six"
                                style="@style/ButtonGrayLight"
                                android:layout_width="80dp"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="ETH" />
                            <View
                                android:layout_width="5dp"
                                android:layout_height="@dimen/line" />

                            <com.black.base.widget.SpanCheckedTextView
                                android:id="@+id/sven"
                                android:layout_width="100dp"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:textColor="#E622C177"
                                android:padding="@dimen/middle_padding"
                                android:text="@string/more" />


                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/text_size_10"
                            android:layout_marginLeft="@dimen/text_size_10"
                            android:layout_marginTop="@dimen/text_size_10"
                            android:layout_marginBottom="@dimen/text_size_10"
                            android:background="@drawable/bg_edittext_default"
                            android:padding="0dp"
                            android:maxLines="1"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="35dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:orientation="horizontal">
                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/text_size_10"
                                    android:orientation="vertical">
                                    <com.black.base.widget.SpanCheckedTextView
                                        android:id="@+id/amount"
                                        android:layout_width="150dp"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:checked="true"
                                        android:textSize="@dimen/text_size_13"
                                        android:textColor="@color/c2c_tab_buy"
                                        android:textStyle="bold"
                                        android:text="@string/purchase_by_amount"/>

                                    <View
                                        android:id="@+id/bar_a"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/text_size_5"
                                        android:layout_width="40dp"
                                        android:layout_height="4dp"
                                        android:background="#E622C177"
                                        />
                                </LinearLayout>
                                <View
                                    android:id="@+id/view1"
                                    android:layout_width="20dp"
                                    android:layout_height="wrap_content"
                                    />
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/text_size_10"
                                    android:orientation="vertical">
                                    <com.black.base.widget.SpanCheckedTextView
                                        android:id="@+id/accont"
                                        android:layout_width="150dp"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:textSize="@dimen/text_size_13"
                                        android:textColor="@color/c2c_tab_buy"
                                        android:textStyle="bold"
                                        android:text="@string/purchase_by_account"/>
                                    <View
                                        android:id="@+id/bar_b"
                                        android:layout_width="40dp"
                                        android:visibility="gone"
                                        android:layout_marginTop="@dimen/text_size_5"
                                        android:layout_gravity="center"
                                        android:layout_height="4dp"
                                        android:background="#E622C177"
                                        />
                                </LinearLayout>

                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginRight="@dimen/text_size_10"
                                android:layout_marginLeft="@dimen/text_size_10"
                                android:background="@color/C2"/>
                            <com.black.lib.view.InputLinearLayout
                                android:id="@+id/money"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/text_size_10"
                                android:layout_marginLeft="@dimen/text_size_10"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:layout_marginBottom="@dimen/text_size_10"
                                android:background="@drawable/bg_edittext_default"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">
                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/get_mail_code"
                                    style="@style/EditTextStyle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="￥"
                                    android:textColor="@color/T1"
                                    android:textStyle="bold"
                                    android:textSize="@dimen/text_size_16" />

                            <com.black.base.widget.SpanMaterialEditText
                                android:id="@+id/put_money"
                                style="@style/EditTextStyle"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@null"
                                android:hint="@string/example"
                                android:inputType="number"
                                android:maxLines="1"
                                android:padding="0dp" />

                            </com.black.lib.view.InputLinearLayout>
                            <com.black.lib.view.InputLinearLayout
                                android:id="@+id/money_account"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginRight="@dimen/text_size_10"
                                android:layout_marginLeft="@dimen/text_size_10"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:layout_marginBottom="@dimen/text_size_10"
                                android:visibility="gone"
                                android:background="@drawable/bg_edittext_default"
                                android:orientation="horizontal">
                                <com.black.base.widget.SpanMaterialEditText
                                    style="@style/EditTextStyle"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@null"
                                    android:hint="@string/example_most"
                                    android:inputType="number"
                                    android:maxLines="1"
                                    android:padding="0dp" />
                            </com.black.lib.view.InputLinearLayout>
                            <LinearLayout
                                android:id="@+id/money_amount"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/text_size_20"
                                android:layout_marginLeft="@dimen/text_size_20"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:layout_marginBottom="@dimen/text_size_10"
                                android:orientation="horizontal">

                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/first"
                                    style="@style/ButtonGray"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:text="1x" />

                                <View
                                    android:layout_width="5dp"
                                    android:layout_height="@dimen/line" />

                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/second"
                                    style="@style/ButtonGray"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:text="5x" />
                                <View
                                    android:layout_width="5dp"
                                    android:layout_height="@dimen/line" />

                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/third"
                                    style="@style/ButtonGray"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:text="10x" />
                                <View
                                    android:layout_width="5dp"
                                    android:layout_height="@dimen/line" />

                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/fourth"
                                    style="@style/ButtonGray"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:text="20x" />
                                <View
                                    android:layout_width="5dp"
                                    android:layout_height="@dimen/line" />

                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/fifth"
                                    style="@style/ButtonGray"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:text="50x" />
                                <View
                                    android:layout_width="5dp"
                                    android:layout_height="@dimen/line" />

                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/sixth"
                                    style="@style/ButtonGray"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:text="100x" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/advice"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:layout_marginRight="@dimen/text_size_20"
                                android:layout_marginLeft="@dimen/text_size_20"
                                android:orientation="horizontal">
                                <com.black.base.widget.SpanTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"

                                    android:text="@string/reference_quotation" />
                                <com.black.base.widget.SpanTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:layout_weight="1"
                                    android:textColor="@color/C5"
                                    android:text="@string/advice" />
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/advice_null"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="#FAF3DD"
                                android:visibility="gone"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:layout_marginRight="@dimen/text_size_20"
                                android:layout_marginLeft="@dimen/text_size_20"
                                android:orientation="horizontal">
                               <ImageView
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:layout_marginTop="@dimen/text_size_5"
                                   android:layout_marginLeft="@dimen/text_size_10"
                                   android:layout_marginRight="@dimen/text_size_10"
                                   android:background="@drawable/icon_secure_tip"/>
                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/advice_all"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/text_size_5"
                                    android:layout_marginBottom="@dimen/text_size_5"
                                    android:textColor="@color/C5"
                                    android:layout_weight="1"
                                    android:text="@string/advice_null" />
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/pay_account"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/text_size_20"
                                android:layout_marginRight="@dimen/text_size_20"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:orientation="horizontal">
                                <com.black.base.widget.SpanTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:text="@string/number_of_transactions" />
                                <com.black.base.widget.SpanTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:layout_weight="1"
                                    android:text="-USDT" />
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/payment_method"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/text_size_10"
                                android:layout_marginLeft="@dimen/text_size_20"
                                android:layout_marginRight="@dimen/text_size_20"
                                android:orientation="horizontal">
                                <com.black.base.widget.SpanTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:text="@string/payment_method" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/id_pay_layout"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginLeft="@dimen/text_size_20"
                                android:layout_marginRight="@dimen/text_size_20"
                                style="@style/ButtonGray"
                                android:layout_marginTop="@dimen/text_size_5"
                                android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/id_pay_Image"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/text_size_10"
                                    />
                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/id_pay_id"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/text_size_5"
                                    android:layout_marginTop="6dp"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:layout_gravity="center_vertical"
                                    android:text="@string/cards" />
                                <com.black.base.widget.SpanTextView
                                    android:id="@+id/id_pay"
                                    android:layout_width="220dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/text_size_20"
                                    android:layout_marginTop="6dp"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:layout_gravity="center_vertical"
                                    android:text="6222222222222" />
                            <ImageView
                                android:id="@+id/id_pay_concrete"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/text_size_10"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/arrow_right"/>
                            </LinearLayout>
                            <com.black.base.widget.SpanTextView
                                android:id="@+id/btn_confirm"
                                style="@style/ButtonBlue"
                                android:layout_width="300dp"
                                android:layout_marginTop="@dimen/text_size_20"
                                android:layout_marginBottom="@dimen/text_size_20"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:padding="@dimen/middle_padding"
                                android:text="@string/by_0_fee" />
                            <com.black.base.widget.SpanTextView
                                android:id="@+id/btn_confirm_sale"
                                style="@style/ButtonRed"
                                android:layout_width="300dp"
                                android:layout_marginTop="@dimen/text_size_20"
                                android:layout_marginBottom="@dimen/text_size_20"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                               android:visibility="gone"
                                android:padding="@dimen/middle_padding"
                                android:text="@string/sale_0_fee" />
                        </LinearLayout>
                        <com.black.base.widget.SpanTextView
                            android:id="@+id/quilk_point"
                            android:layout_width="400dp"
                            android:layout_marginTop="@dimen/text_size_20"
                            android:background="#F4FDF4"
                            android:layout_height="60dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="@dimen/text_size_10"
                            android:layout_gravity="center"
                            android:drawableLeft="@drawable/quilk_point"
                            android:drawableRight="@drawable/icon_setting_arrow_down"
                            android:padding="@dimen/text_size_15"
                            android:textSize="@dimen/text_size_18"
                            android:textColor="@color/black"
                            android:text="@string/buy_quilk" />
                        <com.black.base.widget.SpanTextView
                            android:id="@+id/quilk_point_sale"
                            android:layout_width="400dp"
